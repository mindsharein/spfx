    "basic-form-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/basicForm/BasicFormWebPart.js",
          "manifest": "./src/webparts/basicForm/BasicFormWebPart.manifest.json"
        }
      ]
